<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/utama">

    <fragment
        android:id="@+id/utama"
        android:name="org.d3if1071.materibph.Utama"
        android:label="Utama" >
        <action
            android:id="@+id/action_utama_to_menuDua"
            app:destination="@id/menuDua" />
        <action
            android:id="@+id/action_utama_to_menuSatu"
            app:destination="@id/menuSatu" />
        <action
            android:id="@+id/action_utama_to_about"
            app:destination="@id/about" />
    </fragment>
    <fragment
        android:id="@+id/menuDua"
        android:name="org.d3if1071.materibph.MenuDua"
        android:label="MenuDua" />
    <fragment
        android:id="@+id/menuSatu"
        android:name="org.d3if1071.materibph.MenuSatu"
        android:label="MenuSatu" />
    <fragment
        android:id="@+id/about"
        android:name="org.d3if1071.materibph.About"
        android:label="@string/about"
        tools:layout="@layout/fragment_about" />
</navigation>